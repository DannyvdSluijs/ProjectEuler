{
    "name": "dannyvandersluijs/project-euler",
    "description": "Solutions to https://projecteuler.net",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dannyvandersluijs\\ProjectEuler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Dannyvandersluijs\\ProjectEuler\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Danny van der Sluijs",
            "email": "danny.vandersluijs@icloud.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-bcmath": "*",
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "scripts": {
        "code-style": "phpcs src tests --report=full --standard=PSR12",
        "code-style-fix": "phpcbf src tests --report=full --standard=PSR12"
    }
}
